    <!-- First Column should be here. 1x5-->
    <div class="firstcolumn">
      <table>
        <thead></thead>
        <tbody>
          <tr>
            <td><span>dd</span></td>
          </tr>
          <tr>
            <td><span>dd</span></td>
          </tr>
          <tr>
            <td><span>dd</span></td>
          </tr>
          <tr>
            <td><span>dd</span></td>
          </tr>
          <tr>
            <td><span>dd</span></td>
          </tr>
        </tbody>
      </table>
    </div>
    <!-- First 5x5 table for the Swarachakra keys. 5x5-->
    <div class="maintable1">
      <table>
        <thead></thead>
        <tbody>
          <tr ng-repeat="data in onscreen | limitTo: 5">
            <td ng-repeat="row in data " data-keycode="{{ row.keycode }}" data-chakra="{{ row.chakra }}">
              <span>{{ row.keycode }}</span>
            </td>
          </tr>
        </tbody>
      </table>
    </div>
    <!-- Second 5x5 table for the Swarachakra keys. 5x5-->
    <div class="maintable2">
      <table>
        <thead></thead>
        <tbody>
          <tr>
            <td ng-repeat="row in onscreen[5]" data-keycode="{{ row.keycode }}" data-chakra="{{ row.chakra }}" ng-click="share(row.unicode)">
              <span>{{ row.keycode }}</span>
            </td>
          </tr>
          <tr>
            <td ng-repeat="row in onscreen[6]" data-keycode="{{ row.keycode }}" data-chakra="{{ row.chakra }}">
              <span>{{ row.keycode }}</span>
            </td>
          </tr>
          <tr>
            <td ng-repeat="row in onscreen[7]" data-keycode="{{ row.keycode }}" data-chakra="{{ row.chakra }}">
              <span>{{ row.keycode }}</span>
            </td>
          </tr>
          <tr>
            <td ng-repeat="row in onscreen[8]" data-keycode="{{ row.keycode }}" data-chakra="{{ row.chakra }}">
              <span>{{ row.keycode }}</span>
            </td>
          </tr>
          <tr>
            <td ng-repeat="row in onscreen[9]" data-keycode="{{ row.keycode }}" data-chakra="{{ row.chakra }}">
              <span>{{ row.keycode }}</span>
            </td>
          </tr>
        </tbody>
      </table>
    </div>
    <!-- Last Column should be here. 1x5 -->
    <div class="lastcolumn">
      <table>
        <thead></thead>
        <tbody>
          <tr>
            <td><span>dd</span></td>
          </tr>
          <tr>
            <td><span>dd</span></td>
          </tr>
          <tr>
            <td><span>dd</span></td>
          </tr>
          <tr>
            <td><span>dd</span></td>
          </tr>
          <tr>
            <td><span>dd</span></td>
          </tr>
        </tbody>
      </table>
    </div>
    <!-- Last row should be here.5x1 -->
    <div class="lastrow">
      <table>
        <thead></thead>
        <tbody>
          <tr>
            <td><span>aa</span></td>
            <td><span>aa</span></td>
            <td><span>aa</span></td>
            <td id="spacebar">
              <span></span>
            </td>
            <td><span>aa</span></td>
            <td><span>aa</span></td>
            <td><span>aa</span></td>
            <td><span>aa</span></td>
          </tr>
        </tbody>
      </table>
    </div>
  </div>
</div>