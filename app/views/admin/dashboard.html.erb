<div class="page-header">
	<h1>The dashboard</h1>
</div>
<table class="table">
	<thead>
		<th>Name</th>
		<th>Path</th>
		<th>Enabled</th>
		<th>Delete</th>
	</thead>
	<tbody>
		<% @languages.each do |language| %>
			<tr>
				<td><%= language.name %></td>
				<td><%= language.path %></td>
				<td><%= language.enabled %></td>
				<td><i class="fa fa-remove"></i></td>
			</tr>
		<% end %>
	</tbody>
</table>