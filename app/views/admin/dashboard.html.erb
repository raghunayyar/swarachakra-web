<div class="page-header">
	<h1>The dashboard</h1>
</div>
<!-- TODO: Add Link to New Page -->
<table class="table">
	<thead>
		<th>Name</th>
		<th>Path</th>
		<th>Enabled</th>
		<th>Delete</th>
	</thead>
	<tbody>
		<% @languages.each do |language| %>
			<tr>
				<td><%= language.name %></td>
				<td><%= language.path %></td>
				<td><%= check_box_tag 'accept', language.enabled, language.enabled %></td>
				<td>
				<button class="btn btn-danger btn-xs" class="deletecsv" id="deletecsv">
					<span class="fa fa-remove"></span>
				</button>
				</td>
			</tr>
		<% end %>
	</tbody>
</table>
